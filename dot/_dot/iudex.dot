  "iudex-filter" -> "rjack-slf4j"
  "iudex-filter" -> "rjack-logback"

  "iudex-http" -> "gravitext-util"
  "iudex-http" -> "rjack-slf4j"

  "iudex-barc" -> "gravitext-util"
  "iudex-barc" -> "iudex-http"
  "iudex-barc" -> "rjack-slf4j"

  "iudex-core" -> "iudex-filter"
